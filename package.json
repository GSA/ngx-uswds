{
  "name": "usa-components",
  "version": "16.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run sandbox-icons && npm run docs:json && storybook dev -p 4200",
    "develop": "ng serve --port 4200",
    "build": "npm run build:components && npm run build:formly",
    "test": "npm run test:components && npm run test:formly",
    "build:components": "ng build uswds-components --configuration production",
    "build:formly": "ng build uswds-formly --configuration production",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:components": "ng test uswds-components --watch=false --code-coverage --no-progress",
    "test:formly": "ng test uswds-formly --watch=false --code-coverage --no-progress",
    "ghpages": "npm run build-storybook && touch storybook-static/.nojekyll && npx angular-cli-ghpages --dir=storybook-static",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "sandbox-icons": "node src/sandbox/generate-icons.js",
    "storybook": "ng run docs:json && ng run usa-components:storybook",
    "build-storybook": "ng run usa-components:build-storybook"
  },
  "private": true,
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  },
  "dependencies": {
    "@angular/animations": "^16.2.9",
    "@angular/common": "^16.2.9",
    "@angular/compiler": "^16.2.9",
    "@angular/core": "^16.2.9",
    "@angular/forms": "^16.2.9",
    "@angular/platform-browser": "^16.2.9",
    "@angular/platform-browser-dynamic": "^16.2.9",
    "@angular/router": "^16.2.9",
    "@gsa-sam/sam-styles": "^3.0.12",
    "@ngx-formly/core": "^6.3.0",
    "core-js": "^3.21.1",
    "ngx-highlightjs": "4.1.3",
    "ngx-markdown": "^16.0.0",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.13.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.6",
    "@angular/cli": "^16.2.6",
    "@angular/compiler-cli": "^16.2.9",
    "@babel/core": "^7.17.8",
    "@compodoc/compodoc": "^1.1.19",
    "@etchteam/storybook-addon-css-variables-theme": "^2.0.6",
    "@storybook/addon-a11y": "^7.6.7",
    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/addon-interactions": "^7.6.7",
    "@storybook/addon-links": "^7.6.7",
    "@storybook/addon-mdx-gfm": "^7.6.7",
    "@storybook/angular": "^7.6.7",
    "@storybook/testing-library": "0.2.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^16.0.0",
    "angular-cli-github-pages": "0.2.0",
    "babel-loader": "^8.2.4",
    "codelyzer": "^6.0.0",
    "css-loader": "^6.5.1",
    "ejs": "^3.1.6",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^16.2.3",
    "protractor": "~7.0.0",
    "raw-loader": "^4.0.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "storybook": "^7.6.7",
    "storybook-addon-preview": "^2.3.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "^4.9.3"
  }
}