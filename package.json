{
  "name": "usa-components",
  "version": "11.0.3",
  "scripts": {
    "ng": "ng",
    "start": "npm run sandbox-icons && npm run docs:json && start-storybook -p 4200",
    "develop": "ng serve --port 4200",
    "build": "npm run build:components && npm run build:formly",
    "test": "npm run test:components && npm run test:formly",
    "build:components": "ng build uswds-components --prod",
    "build:formly": "ng build uswds-formly --prod",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:components": "ng test uswds-components --watch=false --code-coverage --no-progress",
    "test:formly": "ng test uswds-formly --watch=false --code-coverage --no-progress",
    "ghpages": "npm run build-storybook && touch storybook-static/.nojekyll && npx angular-cli-ghpages --dir=storybook-static",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "sandbox-icons": "node src/sandbox/generate-icons.js",
    "storybook": "npm run sandbox-icons && npm run docs:json && start-storybook -p 6006",
    "build-storybook": "npm run docs:json && build-storybook"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.2.14",
    "@angular/common": "~11.2.14",
    "@angular/compiler": "~11.2.14",
    "@angular/core": "~11.2.14",
    "@angular/forms": "~11.2.14",
    "@angular/material": "~10.2.0",
    "@angular/platform-browser": "~11.2.14",
    "@angular/platform-browser-dynamic": "~11.2.14",
    "@angular/router": "~11.2.14",
    "@gsa-sam/sam-styles": "0.0.127-beta.4",
    "@ngx-formly/core": "^5.10.23",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.6.11",
    "ngx-highlightjs": "4.1.3",
    "ngx-markdown": "^11.1.3",
    "rxjs": "^6.6.0",
    "tslib": "^2.0.0",
    "uswds": "2.11.2",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.14",
    "@angular/cli": "~11.2.14",
    "@angular/compiler-cli": "~11.2.14",
    "@angular/elements": "11.2.14",
    "@babel/core": "^7.16.0",
    "@compodoc/compodoc": "^1.1.16",
    "@etchteam/storybook-addon-css-variables-theme": "^1.0.2",
    "@storybook/addon-a11y": "6.4.0",
    "@storybook/addon-actions": "^6.4.0",
    "@storybook/addon-essentials": "^6.4.0",
    "@storybook/addon-links": "^6.4.0",
    "@storybook/angular": "^6.4.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.20.42",
    "@webcomponents/custom-elements": "^1.5.0",
    "angular-cli-github-pages": "0.2.0",
    "babel-loader": "^8.2.3",
    "codelyzer": "^6.0.0",
    "ejs": "3.1.6",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.2.4",
    "prismjs": "1.26.0",
    "protractor": "~7.0.0",
    "react-element-to-jsx-string": "^14.3.4",
    "storybook-addon-preview": "^2.2.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
}
