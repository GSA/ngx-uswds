{
  "name": "usa-components",
  "version": "12.0.3",
  "scripts": {
    "ng": "ng",
    "start": "npm run sandbox-icons && npm run docs:json && start-storybook -p 4200",
    "develop": "ng serve --port 4200",
    "build": "npm run build:components && npm run build:formly",
    "test": "npm run test:components && npm run test:formly",
    "build:components": "ng build uswds-components --prod",
    "build:formly": "ng build uswds-formly --prod",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:components": "ng test uswds-components --watch=false --code-coverage --no-progress",
    "test:formly": "ng test uswds-formly --watch=false --code-coverage --no-progress",
    "ghpages": "npm run build-storybook && touch storybook-static/.nojekyll && npx angular-cli-ghpages --dir=storybook-static",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "sandbox-icons": "node src/sandbox/generate-icons.js",
    "storybook": "npm run sandbox-icons && npm run docs:json && start-storybook -p 6006 --no-open",
    "build-storybook": "npm run docs:json && build-storybook"
  },
  "private": true,
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  },
  "dependencies": {
    "@angular/animations": "~12.2.14",
    "@angular/common": "~12.2.14",
    "@angular/compiler": "~12.2.14",
    "@angular/core": "~12.2.14",
    "@angular/forms": "~12.2.14",
    "@angular/platform-browser": "~12.2.14",
    "@angular/platform-browser-dynamic": "~12.2.14",
    "@angular/router": "~12.2.14",
    "@gsa-sam/sam-styles": "0.0.143",
    "@ngx-formly/core": "^5.10.23",
    "classlist.js": "^1.1.20150312",
    "core-js": "^3.21.1",
    "ngx-highlightjs": "4.1.3",
    "ngx-markdown": "^12.0.1",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "uswds": "2.11.2",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.14",
    "@angular/cli": "~12.2.14",
    "@angular/compiler-cli": "~12.2.14",
    "@babel/core": "^7.17.8",
    "@compodoc/compodoc": "^1.1.19",
    "@etchteam/storybook-addon-css-variables-theme": "^1.1.1",
    "@storybook/addon-a11y": "^6.4.20",
    "@storybook/addon-actions": "^6.4.20",
    "@storybook/addon-essentials": "^6.4.20",
    "@storybook/addon-interactions": "^6.4.20",
    "@storybook/addon-links": "^6.4.20",
    "@storybook/angular": "^6.4.20",
    "@storybook/builder-webpack5": "^6.4.20",
    "@storybook/manager-webpack5": "^6.4.20",
    "@storybook/testing-library": "0.0.9",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "angular-cli-github-pages": "0.2.0",
    "babel-loader": "^8.2.4",
    "codelyzer": "^6.0.0",
    "css-loader": "^6.5.1",
    "ejs": "^3.1.6",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.4",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^12.2.6",
    "protractor": "~7.0.0",
    "raw-loader": "^4.0.2",
    "storybook-addon-preview": "^2.2.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.3.5"
  }
}
